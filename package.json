{
  "name": "lib-jitsi-meet",
  "version": "0.0.0",
  "description": "JS library for accessing Jitsi server side deployments",
  "repository": {
    "type": "git",
    "url": "git://https://github.com/internxt/lib-jitsi-meet"
  },
  "keywords": [
    "jingle",
    "webrtc",
    "xmpp",
    "browser",
    "jitsi"
  ],
  "author": "Internxt",
  "readmeFilename": "README.md",
  "dependencies": {
    "@dashlane/pqc-kem-kyber512-browser": "^1.0.0",
    "@jitsi/js-utils": "^2.2.1",
    "@jitsi/logger": "^2.0.2",
    "@jitsi/precall-test": "^1.0.6",
    "@jitsi/rtcstats": "^9.7.0",
    "@jitsi/sdp-interop": "git+https://github.com/jitsi/sdp-interop#3d49eb4aa26863a3f8d32d7581cdb4321244266b",
    "@rollup/rollup-darwin-x64": "^4.34.4",
    "@testrtc/watchrtc-sdk": "^1.38.2",
    "@types/webrtc": "^0.0.43",
    "async-es": "^3.2.4",
    "base64-js": "^1.3.1",
    "buffer": "^6.0.3",
    "current-executing-script": "^0.1.3",
    "jquery": "^3.6.1",
    "lodash-es": "^4.17.21",
    "prettier": "^3.4.2",
    "sdp-transform": "^2.3.0",
    "strophe.js": "^1.6.2",
    "strophejs-plugin-disco": "^0.0.2",
    "strophejs-plugin-stream-management": "git+https://github.com/jitsi/strophejs-plugin-stream-management#679be5902097ed612fb5062b5549f3f32b6f5f47",
    "uuid": "^8.1.0",
    "vite": "^6.1.0",
    "webrtc-adapter": "^8.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.7",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/async": "^3.2.12",
    "@types/babel__preset-env": "^7.9.7",
    "@types/jasmine": "^4.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.4",
    "@types/sdp-transform": "^2.4.5",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "babel-loader": "^8.2.3",
    "core-js": "^3.19.1",
    "eslint": "^9.19.0",
    "jasmine-core": "^4.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "process": "^0.11.10",
    "string-replace-loader": "^3.0.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5",
    "webpack": "^5.97.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.0"
  },
  "scripts": {
    "build": "npm run build:webpack && npm run build:tsc",
    "build:webpack": "webpack",
    "build:webpack-dev": "webpack --mode development",
    "build:tsc": "tsc --build --clean && tsc",
    "gen-types": "tsc --declaration --emitDeclarationOnly --outFile types/index.d.ts",
    "lint-fix": "eslint . --fix",
    "prepack": "npm run build && npm run gen-types",
    "test-karma": "karma start karma.conf.js",
    "test": "vitest",
    "test-watch": "karma start karma.conf.js --no-single-run",
    "validate": "npm ls",
    "watch": "webpack --config webpack.config.js --watch --mode development",
    "format": "prettier \"{src,test}/**/*.{ts,tsx}\" --write",
    "lint:ts": "eslint \"./{src,test}/**/*.{ts,tsx}\"",
    "lint:ts:fix": "npm run lint:ts --fix",
    "lint:scss": "stylelint \"src/**/*.{css,scss}\"",
    "lint:scss:fix": "npm run lint:scss --fix",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "postinstall": "mkdir -p public/libs && cp node_modules/@dashlane/pqc-kem-kyber512-browser/dist/pqc-kem-kyber512.wasm dist/umd"
  },
  "browser": {
    "dist/umd/lib-jitsi-meet.min.js": "dist/umd/lib-jitsi-meet.min.js",
    "pqc-kem-kyber512.wasm": "dist/umd/pqc-kem-kyber512.wasm"
  },

  "module": "dist/esm/JitsiMeetJS.js",
  "files": [
    "dist",
    "types",
    "index.d.ts"
  ],
  "license": "Apache-2.0"
}
