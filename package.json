{
    "name": "lib-jitsi-meet",
    "version": "0.0.5-alpha",
    "description": "JS library for accessing Jitsi server side deployments",
    "repository": {
        "type": "git",
        "url": "git://github.com/jitsi/lib-jitsi-meet"
    },
    "keywords": [
        "jingle",
        "webrtc",
        "xmpp",
        "browser",
        "jitsi"
    ],
    "author": "",
    "readmeFilename": "README.md",
    "dependencies": {
        "@jitsi/js-utils": "2.2.1",
        "@jitsi/logger": "2.0.2",
        "@jitsi/precall-test": "1.0.6",
        "@jitsi/rtcstats": "9.7.0",
        "@testrtc/watchrtc-sdk": "1.38.2",
        "async-es": "3.2.4",
        "current-executing-script": "0.1.3",
        "internxt-crypto": "https://github.com/internxt/crypto/releases/download/v.0.0.5-alpha/internxt-crypto-0.0.5-alpha.tgz",
        "jquery": "3.6.1",
        "lodash-es": "4.17.21",
        "onnxruntime-web": "^1.21.0",
        "patch-package": "6.5.1",
        "sdp-transform": "2.3.0",
        "strophe.js": "1.5.0",
        "strophejs-plugin-disco": "0.0.2",
        "strophejs-plugin-stream-management": "git+https://github.com/jitsi/strophejs-plugin-stream-management#679be5902097ed612fb5062b5549f3f32b6f5f47",
        "uuid": "8.1.0",
        "vodozemac-wasm": "git+https://github.com/internxt/vodozemac-wasm",
        "webrtc-adapter": "8.1.1"
    },
    "devDependencies": {
        "@babel/core": "7.16.0",
        "@babel/preset-env": "7.16.0",
        "@babel/preset-typescript": "7.16.7",
        "@jitsi/eslint-config": "6.0.4",
        "@types/async": "3.2.12",
        "@types/jasmine": "4.0.3",
        "@types/lodash-es": "4.17.12",
        "@types/sdp-transform": "2.4.5",
        "babel-loader": "8.2.3",
        "buffer": "^6.0.3",
        "core-js": "3.19.1",
        "crypto-browserify": "^3.12.1",
        "eslint": "8.57.0",
        "jasmine-core": "4.2.0",
        "karma": "6.4.0",
        "karma-chrome-launcher": "3.1.1",
        "karma-jasmine": "5.1.0",
        "karma-sourcemap-loader": "0.3.8",
        "karma-webpack": "5.0.0",
        "process": "0.11.10",
        "stream-browserify": "^3.0.0",
        "string-replace-loader": "3.0.3",
        "ts-loader": "^9.5.2",
        "ts-mockito": "^2.6.1",
        "typescript": "5.0.4",
        "vm-browserify": "^1.1.2",
        "webpack": "5.76.0",
        "webpack-bundle-analyzer": "4.4.2",
        "webpack-cli": "4.9.0"
    },
    "scripts": {
        "build": "npm run build:webpack && npm run build:tsc",
        "build:webpack": "webpack",
        "build:webpack-dev": "webpack --mode development",
        "build:tsc": "tsc --build --clean && tsc",
        "gen-types": "tsc --declaration --emitDeclarationOnly --outFile types/index.d.ts",
        "lint": "eslint . --max-warnings 0 && tsc --noEmit",
        "lint-fix": "eslint . --max-warnings 0 --fix",
        "postinstall": "patch-package",
        "setup": "mkdir -p public/libs && mkdir -p dist/umd",
        "prepack": "npm run build && npm run gen-types",
        "test": "karma start karma.conf.js",
        "test-watch": "karma start karma.conf.js --no-single-run",
        "validate": "npm ls",
        "watch": "webpack --config webpack.config.js --watch --mode development"
    },
    "browser": {
        "dist/umd/lib-jitsi-meet.min.js": "dist/umd/lib-jitsi-meet.min.js",
        "vodozemac.wasm": "dist/umd/vodozemac.wasm"
    },
    "module": "dist/esm/JitsiMeetJS.js",
    "files": [
        "dist",
        "types",
        "index.d.ts",
        "models",
        "wasm"
    ],
    "license": "Apache-2.0"
}
